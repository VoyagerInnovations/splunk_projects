<form script="uap.js" stylesheet="uap.css" theme="dark">
  <label>PayMaya Issuing Aftersales Portal</label>
  <fieldset submitButton="true" autoRun="false">
    <input type="text" id="input_jira" token="tkn_jira">
      <label>Enter Case Number (alphanumeric/hyphen CASE-00001)</label>
    </input>
  </fieldset>
  <search>
      <done>
          <condition>
              <set token="panelresult_show">true</set>
          </condition>
      </done>
    <query>
                 | makeresults 
                 | eval currentUser= $currentUser|s$
                 | eval jira= $tkn_jira|s$
                 | eval action= $myToken|s$
                 | eval min= $tkn_min|s$
                 | eval lname= $tkn_lName|s$
                 | eval fname= $tkn_fName|s$
                 | eval oldMin= $tkn_oldMin|s$
                 | eval newMin= $tkn_newMin|s$
                 | eval kycId= " "
                 | eval updateProfile= $myToken2|s$
                 | eval updateFName= " "
                 | eval updateMName= " "
                 | eval updateLName= " "
                 | eval updateAddressName= " "
                 | eval updateAddressValue= " "
                 | eval updateAddressType= " "
                 | eval updateBirthday= " "
                 | eval updateEAddress= $tkn_updateEAddress|s$
                 | eval file= $tkn_file|s$
                 | eval srcMin= $tkn_srcMin|s$
                 | eval RRN= $tkn_RRN|s$
                 | table currentUser action jira min lname fname oldMin newMin kycId file srcMin RRN updateProfile updateFName updateMName updateLName updateAddressName updateAddressValue updateAddressType updateBirthday updateEAddress
                 | outputcsv $env:user$.csv 
          </query>
  </search>
