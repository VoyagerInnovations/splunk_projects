  <row>
    <panel depends="$panelresult_show$">
      <title>Result</title>
      <table>
        <search>
          <query>| inputlookup updateprofile_$env:user$_result.csv</query>
          <earliest>-15m</earliest>
          <latest>now</latest>
          <refresh>5s</refresh>
          <refreshType>delay</refreshType>
        </search>
        <option name="count">100</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
        <option name="wrap">false</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>Audit Log</title>
      <input type="dropdown" token="timerange_from" searchWhenChanged="true">
        <label>Date From</label>
        <fieldForLabel>label</fieldForLabel>
        <fieldForValue>value</fieldForValue>
        <search>
          <query> | gentimes start=1/1/20 end=+1 
                  | eval label=strftime(starttime, "%Y-%b-%d") 
                  | eval value="earliest=".'starttime'
                  | sort -endtime
          </query>
          <earliest>-15m</earliest>
          <latest>now</latest>
        </search>
      </input>
      <input type="dropdown" token="timerange_to" searchWhenChanged="true">
        <label>Date To</label>
        <fieldForLabel>label</fieldForLabel>
        <fieldForValue>value</fieldForValue>
        <search>
          <query> | gentimes start=1/1/20 end=+1
                  | eval label=strftime(starttime, "%Y-%b-%d")
                  | eval value="latest=".'endtime'
                  | sort -endtime
          </query>
          <earliest>-15m</earliest>
          <latest>now</latest>
        </search>
      </input>
      <input type="text" token="username" searchWhenChanged="true">
        <label>Username</label>
        <default>*</default>
        <prefix>USERNAME="*</prefix>
        <suffix>*"</suffix>
        <initialValue>*</initialValue>
      </input>
      <input type="text" token="jiraticket" searchWhenChanged="true">
        <label>Ticket  Reference</label>
        <prefix>TICKET="*</prefix>
        <suffix>*"</suffix>
        <initialValue>*</initialValue>
        <default>*</default>
      </input>
      <input type="text" token="detailsmin" searchWhenChanged="true">
        <label>Details</label>
        <prefix>DETAILS="*</prefix>
        <suffix>*"</suffix>
        <initialValue>*</initialValue>
        <default>*</default>
      </input>
      <table>
        <search>
          <query>index="main" $timerange_from$ $timerange_to$ sourcetype="aftersales_audit_trail" | transaction startswith="TIMESTAMP" | search $username$ $jiraticket$ $detailsmin$ | table _time USERNAME TICKET ACTION DETAILS STATUS</query>
          <earliest>0</earliest>
          <latest></latest>
          <sampleRatio>1</sampleRatio>
          <refresh>1m</refresh>
          <refreshType>delay</refreshType>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
